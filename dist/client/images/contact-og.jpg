<svg width="1200" height="630" viewBox="0 0 1200 630" fill="none" xmlns="http://www.w3.org/2000/svg">
  <defs>
    <linearGradient id="contactGradient" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#7C3AED;stop-opacity:1" />
      <stop offset="50%" style="stop-color:#A855F7;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#6B21A8;stop-opacity:1" />
    </linearGradient>
    <linearGradient id="contactAccent" x1="0%" y1="0%" x2="100%" y2="0%">
      <stop offset="0%" style="stop-color:#10B981;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#059669;stop-opacity:1" />
    </linearGradient>
  </defs>
  
  <!-- Background -->
  <rect width="1200" height="630" fill="url(#contactGradient)"/>
  
  <!-- Communication waves -->
  <g opacity="0.1" stroke="white" stroke-width="2" fill="none">
    <circle cx="600" cy="315" r="100"/>
    <circle cx="600" cy="315" r="150"/>
    <circle cx="600" cy="315" r="200"/>
    <circle cx="600" cy="315" r="250"/>
  </g>
  
  <!-- Main logo -->
  <g transform="translate(100, 80)">
    <path d="M30 10L20 15V25C20 35 25 45 30 47.5C35 45 40 35 40 25V15L30 10Z" fill="url(#contactAccent)"/>
    <rect x="27" y="22" width="6" height="5" rx="0.5" fill="white" opacity="0.9"/>
    <text x="60" y="25" font-family="Inter, sans-serif" font-size="32" font-weight="700" fill="white">SESEC</text>
    <text x="60" y="45" font-family="Inter, sans-serif" font-size="14" font-weight="400" fill="#E9D5FF">CONTACT US</text>
  </g>
  
  <!-- Main title -->
  <text x="100" y="200" font-family="Inter, sans-serif" font-size="56" font-weight="800" fill="white">Hablemos de tu</text>
  <text x="100" y="260" font-family="Inter, sans-serif" font-size="56" font-weight="800" fill="url(#contactAccent)">Seguridad</text>
  
  <!-- Contact methods -->
  <g transform="translate(700, 180)">
    <!-- Email -->
    <g>
      <rect x="0" y="0" width="100" height="80" rx="8" fill="white" opacity="0.1"/>
      <rect x="20" y="25" width="60" height="30" rx="4" fill="url(#contactAccent)" opacity="0.8"/>
      <path d="M20 25L50 40L80 25" stroke="white" stroke-width="2" fill="none"/>
      <text x="50" y="95" text-anchor="middle" font-family="Inter, sans-serif" font-size="12" fill="white">Email</text>
    </g>
    
    <!-- Phone -->
    <g transform="translate(130, 0)">
      <rect x="0" y="0" width="100" height="80" rx="8" fill="white" opacity="0.1"/>
      <rect x="30" y="15" width="40" height="50" rx="8" fill="url(#contactAccent)" opacity="0.8"/>
      <rect x="35" y="20" width="30" height="20" rx="2" fill="white" opacity="0.3"/>
      <circle cx="50" cy="55" r="3" fill="white"/>
      <text x="50" y="95" text-anchor="middle" font-family="Inter, sans-serif" font-size="12" fill="white">Teléfono</text>
    </g>
    
    <!-- Meeting -->
    <g transform="translate(260, 0)">
      <rect x="0" y="0" width="100" height="80" rx="8" fill="white" opacity="0.1"/>
      <rect x="25" y="20" width="50" height="35" rx="4" fill="url(#contactAccent)" opacity="0.8"/>
      <circle cx="40" cy="30" r="4" fill="white"/>
      <circle cx="60" cy="30" r="4" fill="white"/>
      <path d="M30 45L70 45" stroke="white" stroke-width="2"/>
      <text x="50" y="95" text-anchor="middle" font-family="Inter, sans-serif" font-size="12" fill="white">Reunión</text>
    </g>
  </g>
  
  <!-- Contact info -->
  <g transform="translate(100, 320)">
    <text font-family="Inter, sans-serif" font-size="24" font-weight="600" fill="white">📧 info@sesec.es</text>
    <text y="50" font-family="Inter, sans-serif" font-size="24" font-weight="600" fill="white">📞 +34 900 123 456</text>
    <text y="100" font-family="Inter, sans-serif" font-size="24" font-weight="600" fill="white">🏢 Madrid, España</text>
    <text y="150" font-family="Inter, sans-serif" font-size="20" font-weight="400" fill="#E9D5FF">Respuesta garantizada en 24 horas</text>
  </g>
  
  <!-- Security badge -->
  <g transform="translate(900, 400)">
    <circle cx="50" cy="50" r="40" fill="url(#contactAccent)" opacity="0.2"/>
    <path d="M50 20L35 25V40C35 50 42.5 60 50 62.5C57.5 60 65 50 65 40V25L50 20Z" fill="url(#contactAccent)"/>
    <rect x="46" y="38" width="8" height="6" rx="1" fill="white"/>
    <path d="M48 38V35C48 33 49 32 50 32C51 32 52 33 52 35V38" stroke="white" stroke-width="1.5" fill="none"/>
    <text x="50" y="110" text-anchor="middle" font-family="Inter, sans-serif" font-size="12" fill="white">Comunicación</text>
    <text x="50" y="125" text-anchor="middle" font-family="Inter, sans-serif" font-size="12" fill="white">Segura</text>
  </g>
  
  <!-- Bottom accent -->
  <rect x="0" y="600" width="1200" height="30" fill="url(#contactAccent)"/>
</svg>