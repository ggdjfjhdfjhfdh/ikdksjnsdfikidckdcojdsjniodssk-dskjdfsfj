<svg width="1200" height="630" viewBox="0 0 1200 630" fill="none" xmlns="http://www.w3.org/2000/svg">
  <defs>
    <linearGradient id="servicesGradient" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#1E40AF;stop-opacity:1" />
      <stop offset="50%" style="stop-color:#3B82F6;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#1E3A8A;stop-opacity:1" />
    </linearGradient>
    <linearGradient id="serviceAccent" x1="0%" y1="0%" x2="100%" y2="0%">
      <stop offset="0%" style="stop-color:#10B981;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#059669;stop-opacity:1" />
    </linearGradient>
  </defs>
  
  <!-- Background -->
  <rect width="1200" height="630" fill="url(#servicesGradient)"/>
  
  <!-- Circuit pattern -->
  <g opacity="0.1" stroke="white" stroke-width="1" fill="none">
    <path d="M100 100L200 100L200 200L300 200M300 200L400 200L400 300M500 150L600 150L600 250L700 250"/>
    <path d="M800 100L900 100L900 200L1000 200M200 300L300 300L300 400L400 400M600 350L700 350L700 450"/>
    <circle cx="200" cy="100" r="3" fill="white"/>
    <circle cx="300" cy="200" r="3" fill="white"/>
    <circle cx="600" cy="150" r="3" fill="white"/>
    <circle cx="900" cy="100" r="3" fill="white"/>
  </g>
  
  <!-- Main logo -->
  <g transform="translate(100, 80)">
    <path d="M30 10L20 15V25C20 35 25 45 30 47.5C35 45 40 35 40 25V15L30 10Z" fill="url(#serviceAccent)"/>
    <rect x="27" y="22" width="6" height="5" rx="0.5" fill="white" opacity="0.9"/>
    <text x="60" y="25" font-family="Inter, sans-serif" font-size="32" font-weight="700" fill="white">SESEC</text>
    <text x="60" y="45" font-family="Inter, sans-serif" font-size="14" font-weight="400" fill="#BFDBFE">CYBERSECURITY SERVICES</text>
  </g>
  
  <!-- Main title -->
  <text x="100" y="200" font-family="Inter, sans-serif" font-size="56" font-weight="800" fill="white">Servicios de</text>
  <text x="100" y="260" font-family="Inter, sans-serif" font-size="56" font-weight="800" fill="url(#serviceAccent)">Ciberseguridad</text>
  
  <!-- Service icons grid -->
  <g transform="translate(700, 180)">
    <!-- Pentesting -->
    <g>
      <rect x="0" y="0" width="80" height="80" rx="8" fill="white" opacity="0.1"/>
      <path d="M20 20L40 10L60 20L50 40L30 40Z" fill="url(#serviceAccent)" opacity="0.8"/>
      <circle cx="40" cy="30" r="3" fill="white"/>
      <text x="40" y="95" text-anchor="middle" font-family="Inter, sans-serif" font-size="12" fill="white">Pentesting</text>
    </g>
    
    <!-- Auditoría -->
    <g transform="translate(120, 0)">
      <rect x="0" y="0" width="80" height="80" rx="8" fill="white" opacity="0.1"/>
      <rect x="20" y="15" width="40" height="50" rx="4" fill="url(#serviceAccent)" opacity="0.8"/>
      <path d="M25 25L55 25M25 35L55 35M25 45L45 45" stroke="white" stroke-width="2"/>
      <circle cx="50" cy="50" r="3" fill="white"/>
      <text x="40" y="95" text-anchor="middle" font-family="Inter, sans-serif" font-size="12" fill="white">Auditoría</text>
    </g>
    
    <!-- Consultoría -->
    <g transform="translate(240, 0)">
      <rect x="0" y="0" width="80" height="80" rx="8" fill="white" opacity="0.1"/>
      <circle cx="40" cy="30" r="15" fill="url(#serviceAccent)" opacity="0.8"/>
      <path d="M30 25L35 30L50 15" stroke="white" stroke-width="3" fill="none"/>
      <text x="40" y="95" text-anchor="middle" font-family="Inter, sans-serif" font-size="12" fill="white">Consultoría</text>
    </g>
    
    <!-- Formación -->
    <g transform="translate(360, 0)">
      <rect x="0" y="0" width="80" height="80" rx="8" fill="white" opacity="0.1"/>
      <rect x="15" y="20" width="50" height="35" rx="3" fill="url(#serviceAccent)" opacity="0.8"/>
      <path d="M25 30L35 35L45 25L55 30" stroke="white" stroke-width="2" fill="none"/>
      <circle cx="40" cy="15" r="4" fill="white"/>
      <text x="40" y="95" text-anchor="middle" font-family="Inter, sans-serif" font-size="12" fill="white">Formación</text>
    </g>
  </g>
  
  <!-- Features list -->
  <g transform="translate(100, 320)">
    <text font-family="Inter, sans-serif" font-size="20" font-weight="600" fill="white">✓ Evaluaciones de seguridad integrales</text>
    <text y="40" font-family="Inter, sans-serif" font-size="20" font-weight="600" fill="white">✓ Implementación de controles avanzados</text>
    <text y="80" font-family="Inter, sans-serif" font-size="20" font-weight="600" fill="white">✓ Capacitación especializada en ciberseguridad</text>
    <text y="120" font-family="Inter, sans-serif" font-size="20" font-weight="600" fill="white">✓ Soporte 24/7 y monitoreo continuo</text>
  </g>
  
  <!-- Bottom accent -->
  <rect x="0" y="600" width="1200" height="30" fill="url(#serviceAccent)"/>
</svg>